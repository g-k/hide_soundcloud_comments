{
  "name": "Hide Soundcloud Comments",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Hide comments on soundcloud dashboard.",
  "icons": {
    "16": "hidden-comment.png"
  },
  "content_scripts": [
    {
      "matches": ["http://soundcloud.com/*"],
      "js": ["jquery-1.8.0.js", "main.js"]
    }
  ]
}
